{
  "name": "kylo",
  "version": "0.8.0",
  "private": true,
  "description": "kylo",
  "scripts": {
    "build": "tsc -p src/main/resources/static/tsconfig.app.json",
    "start": "tsc -p src/main/resources/static/tsconfig.app.json && concurrently \"tsc -p src/main/resources/static -w\" \"lite-server -c src/main/resources/static/bs-config.js\"",
    "reinstall": "rm -rf node_modules tmp deploy dist && npm i",
    "test": "karma start karma.conf.js",
    "test:watch": "tsc -p src/main/resources/static/tsconfig.app.json && concurrently \"tsc -p src/main/resources/static/tsconfig.spec.json -w\" \"karma start karma.conf.js\"",
    "build-css": "node-sass --include-path scss ./src/main/resources/static/assets/medikly_theme/styles/mdk-main.scss ./src/main/resources/static/assets/medikly_theme/styles/css/mdk-main.css",
    "build-css-pricing": "node-sass --include-path scss ./src/main/resources/static/assets/medikly_theme/styles/scss/components/_pricing.scss ./src/main/resources/static/assets/medikly_theme/styles/css/pricing.css",
    "watch-css": "nodemon -e scss -x \"npm run build-css\""
  },
  "engines": {
    "node": ">4.4 < 7",
    "npm": ">3"
  },
  "repository": "https://github.com/teradata/kylo.git",
  "license": "Apache-2.0",
  "author": "Think big Analytics",
  "dependencies": {
    "@angular/animations": "^4.2.0",
    "@angular/common": "^4.2.0",
    "@angular/compiler": "^4.2.0",
    "@angular/core": "^4.2.0",
    "@angular/flex-layout": "^2.0.0-beta.9",
    "@angular/forms": "^4.2.0",
    "@angular/http": "^4.2.0",
    "@angular/material": "2.0.0-beta.6",
    "@angular/platform-browser": "^4.2.0",
    "@angular/platform-browser-dynamic": "^4.2.0",
    "@angular/router": "^4.2.0",
    "@angular/upgrade": "^4.2.0",
    "@covalent/core": "1.0.0-beta.5",
    "@swimlane/ngx-datatable": "9.3.0",
    "ngx-order-pipe": "^1.0.2",
    "ngx-perfect-scrollbar": "^4.5.2",
    "ng2-charts": "1.6.0",
    "ng2-dragula": "1.5.0",
    "ng2-file-upload": "1.2.1",
    "ng2-validation": "4.2.0",
    "ng-circle-progress": "^0.9.5",
    "angular2-autosize": "^1.0.1",
    "angular2-highcharts": "^0.5.5",
    "@types/acorn": "^4.0.2",
    "@uirouter/angular-hybrid": "^3.1.4",
    "angular": "^1.6.2",
    "core-js": "^2.4.1",
    "rxjs": "^5.4.3",
    "systemjs": "^0.19.47",
    "zone.js": "^0.8.17"
  },
  "devDependencies": {
    "@angular/cli": "1.2.6",
    "@angular/compiler-cli": "^4.2.0",
    "@types/angular": "^1.6.32",
    "@types/angular-cookies": "^1.4.5",
    "@types/angular-material": "^1.1.54",
    "@types/angular-mocks": "^1.5.11",
    "@types/angular-sanitize": "^1.3.6",
    "@types/angular-ui-router": "^1.1.38",
    "@types/hammerjs": "2.0.30",
    "@types/jasmine": "2.5.38",
    "@types/jquery": "^2.0.48",
    "@types/requirejs": "^2.1.31",
    "@types/underscore": "^1.8.3",
    "angular-mocks": "^1.6.2",
    "concurrently": "^3.5.0",
    "http-proxy-middleware": "^0.17.4",
    "jasmine-core": "~2.5.2",
    "karma": "~1.4.1",
    "karma-chrome-launcher": "~2.0.0",
    "karma-jasmine": "^1.1.0",
    "karma-jasmine-html-reporter": "^0.2.2",
    "lite-server": "^2.3.0",
    "node-sass": "^4.5.3",
    "nodemon": "^1.12.1",
    "typescript": "~2.3.3"
  }
}
